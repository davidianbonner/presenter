{
    "name": "davidianbonner/presenter",
    "description": "A Laravel presenter and transformer package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "David Bonner",
            "email": "david@continuuminteractive.com"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/contracts": "^6.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit" : "^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DavidIanBonner\\Presenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavidIanBonner\\Presenter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavidIanBonner\\Presenter\\PresenterServiceProvider"
            ]
        }
    }
}
